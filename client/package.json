{
  "name": "vscode-ember-template-lint",
  "description": "VSCode plugin for ember-template-lint",
  "author": "Thomas Sauer",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "t-sauer",
  "engines": {
    "vscode": "^1.8.1"
  },
  "categories": [
    "Linters",
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:handlebars"
  ],
  "main": "./out/src/extension",
  "contributes": {
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.33",
    "@types/node": "^7.0.4",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "dependencies": {
    "vscode-languageclient": "^3.0.3"
  }
}
