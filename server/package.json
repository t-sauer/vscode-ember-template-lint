{
  "name": "ember-template-lint-server",
  "description": "The language server for ember-template-lint.",
  "version": "0.0.1",
  "author": "Thomas Sauer",
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "vscode-languageserver": "^3.0.4"
  },
  "devDependencies": {
    "@types/node": "^7.0.4",
    "typescript": "^2.1.4"
  },
  "scripts": {
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p ."
  }
}
